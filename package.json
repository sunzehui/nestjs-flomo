{
  "name": "nest-flomo",
  "private": true,
  "version": "1.0.0",
  "description": "nest-flomo",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "dev": "cross-env NODE_ENV=development  nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "cross-env NODE_ENV=production  node dist/src/main",
    "start:preview": "nest build cross-env NODE_ENV=preview node dist/src/main",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "dependencies": {
    "@nestjs/cli": "^10.1.11",
    "@nestjs/common": "^10.1",
    "@nestjs/config": "^3.0",
    "@nestjs/core": "^10.1",
    "@nestjs/jwt": "^10.1",
    "@nestjs/mapped-types": "^2.0",
    "@nestjs/passport": "^10.0",
    "@nestjs/platform-express": "^10.1",
    "@nestjs/swagger": "^7.1.8",
    "@nestjs/typeorm": "^10.0.0",
    "@types/express": "^4.17.17",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "moment": "^2.29.4",
    "mysql": "^2.18.1",
    "nestjs-pino": "^3.3.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pino": "^8.15.0",
    "pino-http": "^8.4.0",
    "pino-pretty": "^10.2.0",
    "pino-std-serializers": "^6.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "sqlite3": "^5.1.6",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@darraghor/eslint-plugin-nestjs-typed": "^4.0.11",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.1",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.76",
    "@types/lodash": "^4.14.197",
    "@types/lodash-es": "^4.17.8",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.10",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-sonarjs": "^0.20.0",
    "nest-cli": "^0.0.5",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.4.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6",
    "webpack": "^5.88.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleNameMapper": {
      "@user/(.*)": [
        "<rootDir>/src/user/$1"
      ],
      "@auth/(.*)": [
        "<rootDir>/src/auth/$1"
      ],
      "@(utils|modules)/(.*)": [
        "<rootDir>/src/$1/$2"
      ],
      "@/(.*)$": "<rootDir>/src/$1"
    },
    "rootDir": "./",
    "testRegex": ".*\\.e2e-spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
